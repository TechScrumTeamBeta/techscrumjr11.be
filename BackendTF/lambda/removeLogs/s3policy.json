{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:ListBucketVersions",
                "s3:DeleteObject",
                "s3:DeleteObjectVersion"
            ],
            "Resource": [
                "arn:aws:s3:::techscrum-log",
                "arn:aws:s3:::techscrum-log/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": [ 
                "arn:aws:s3:::techscrum-log",
                "arn:aws:s3:::techscrum-log/*"
            ]
        }
    ]
}